#import "MyAno.h"

@implementation MyAno

@synthesize coordinate;

- (id)initWithLocation:(CLLocationCoordinate2D)coord
{
    self = [super init];
    if (self)
    {
        coordinate = coord;
    }
    
    return self;
    
}


- (void) setCoordinate:(CLLocationCoordinate2D)newCoordinate
{
    
    coordinate = newCoordinate;
}


- (NSString *)title
{
    return @"拖拉以改變地址";
}

// optional
- (NSString *)subtitle
{
    return @"";
}


@end
